	<script src="/login/vendor/animsition/js/animsition.min"></script>
<!--===============================================================================================-->
	<script src="/login/vendor/bootstrap/js/popper"></script>
	<script src="/login/vendor/bootstrap/js/bootstrap.min"></script>
<!--===============================================================================================-->
	<script src="/login/vendor/select2/select2.min"></script>
<!--===============================================================================================-->
	<script src="/login/vendor/daterangepicker/moment.min"></script>
	<script src="/login/vendor/daterangepicker/daterangepicker"></script>
<!--===============================================================================================-->
	<script src="/login/vendor/countdowntime/countdowntime"></script>
<!--===============================================================================================-->
	<script src="/login/js/main"></script>